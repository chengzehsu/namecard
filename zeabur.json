{
  "name": "namecard-telegram-bot",
  "build": {
    "command": "pip install -r requirements-telegram.txt"
  },
  "start": {
    "command": "python main.py"
  },
  "environment": {
    "PORT": "5003",
    "PYTHON_VERSION": "3.9",
    "FLASK_ENV": "production",
    "PYTHONUNBUFFERED": "1"
  },
  "region": "hkg",
  "restart": "always"
}