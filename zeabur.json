  },
  "start": {
    "command": "python telegram_app.py"
  },
  "environment": {
    "PORT": "5003",
    "PYTHON_VERSION": "3.9",
    "FLASK_ENV": "production",
    "PYTHONUNBUFFERED": "1"
  }
}